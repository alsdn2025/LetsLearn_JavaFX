<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.media.*?>

<AnchorPane prefHeight="272.0" prefWidth="332.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="TJ_8_MediaCtrl.RootController">

   <StackPane layoutX="24.0" layoutY="14.0" prefHeight="170.0" prefWidth="270.0">
      <ImageView fitHeight="170.0" fitWidth="270.0" pickOnBounds="true">
         <Image url="@Medias/Image.jpg" />
      </ImageView>
      <MediaView fx:id="mediaView" fitHeight="170.0" fitWidth="270.0" preserveRatio="false" />
   </StackPane>

   <Button fx:id="btnPlay" layoutX="49.0" layoutY="215.0" maxHeight="40.0" maxWidth="40.0" minHeight="40.0" minWidth="40.0" mnemonicParsing="false" prefHeight="40.0" prefWidth="40.0">
      <graphic>
         <ImageView fitHeight="40.0" fitWidth="40.0" nodeOrientation="INHERIT" pickOnBounds="true">
            <Image url="@Medias/BtnImages/PlayImage.jpg" />
         </ImageView>
      </graphic>
   </Button>

   <Button fx:id="btnPause" layoutX="112.0" layoutY="215.0" maxHeight="40.0" maxWidth="40.0" minHeight="40.0" minWidth="40.0" mnemonicParsing="false" prefHeight="40.0" prefWidth="40.0">
      <graphic>
         <ImageView fitHeight="40.0" fitWidth="40.0" nodeOrientation="INHERIT" pickOnBounds="true">
            <Image url="@Medias/BtnImages/PauseImage.jpg" />
         </ImageView>
      </graphic>
   </Button>

   <Button fx:id="btnStop" layoutX="174.0" layoutY="215.0" maxHeight="40.0" maxWidth="40.0" minHeight="40.0" minWidth="40.0" mnemonicParsing="false" prefHeight="40.0" prefWidth="40.0">
      <graphic>
         <ImageView fitHeight="40.0" fitWidth="40.0" nodeOrientation="INHERIT" pickOnBounds="true">
            <Image url="@Medias/BtnImages/StopImage.jpg" />
         </ImageView>
      </graphic>
   </Button>

   <Button fx:id="btnChangeMedia" layoutX="239.0" layoutY="223.0" mnemonicParsing="false" prefHeight="24.0" prefWidth="50.0" text="music" />

   <ProgressBar fx:id="progressBar" layoutX="24.0" layoutY="188.0" prefHeight="8.0" prefWidth="270.0" progress="0.0">
   </ProgressBar>
   <Slider fx:id="sliderTime" blendMode="SOFT_LIGHT" layoutX="21.0" layoutY="180.0" prefHeight="24.0" prefWidth="277.0" />
   <Label fx:id="labelTime" layoutX="24.0" layoutY="197.0" text="0" />

   <Slider fx:id="sliderVolume" layoutX="306.0" layoutY="54.0" orientation="VERTICAL" prefHeight="123.0" prefWidth="14.0" />

   <ImageView fx:id="imageViewVolume" fitHeight="22.0" fitWidth="17.0" layoutX="305.0" layoutY="185.0" pickOnBounds="true" preserveRatio="true">
      <image>
         <Image url="@Medias/VolumeImages/VolumeO.jpg" />
      </image></ImageView>

</AnchorPane>
