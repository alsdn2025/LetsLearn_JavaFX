<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<?import javafx.scene.image.ImageView?>
<?import javafx.geometry.Insets?>

<HBox xmlns="http://javafx.com/javafx" xmlns:fx="http://javafx.com/fxml/1"
      alignment = "CENTER_LEFT"
      style="-fx-background-color:#6d6565; -fx-background-radius: 20">
    <children>
        <ImageView fx:id="imgViewPopup" fitWidth="30" fitHeight="30" preserveRatio="true"/>
        <Label fx:id="lblPopup" style="-fx-text-fill:#ffff00;">
            <HBox.margin>
                <Insets left="50" right="50"/>
            </HBox.margin>
        </Label>
    </children>
</HBox>






